{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
  "moduleResolution": "nodenext",
  "module": "NodeNext",
  "verbatimModuleSyntax": false,
    "paths": {
      "@components/*": ["src/components/*"],
      "@src/*": ["src/*"],
      "@controllers/*": ["src/controllers/*"]
    }
  }
}
